{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "paths": {
      "@/*": ["./src/*"],
      "@src/*": ["./src/*"],
      "@config/*": ["./src/config/*"],
      "@enums/*": ["./src/enums/*"],
      "@entities/*": ["./src/entities/*"],
      "@common/*": ["./src/common/*"],
      "libs/*": ["./libs/*"],
      "components/*": ["./src/components/*"],
      "sharedProject/*": ["./src/sharedProject/*"],
      "sharedArchitech/*": ["./src/sharedArchitech/*"],
      "mui/*": ["./src/mui/*"],
      "resources/*": ["./src/resources/*"],
      "graphql/*": ["./src/graphql/*"],
      "utils/*": ["./src/utils/*"],
      "projectConstants": ["./src/projectConstants"],
      "routes": ["./src/routes"],
      "appContext": ["./src/appContext"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "libs/**/*.ts"],
  "exclude": ["node_modules"]
}
